	keywords = {{EU}, Manufacturing, {PPML}, Structural gravity, Trade potential},
	file = {Full Text PDF:/Users/pepe/Zotero/storage/MNEHVBZQ/Chatzilazarou and Dadakas - 2023 - Trade potential in European Union manufacturing.pdf:application/pdf},
}

